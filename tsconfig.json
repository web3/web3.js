{
	"compilerOptions": {
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"target": "es2016",
		"module": "commonjs",
		"declaration": true,
		"moduleResolution": "node",
		"newLine": "lf",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"removeComments": true,
		"sourceMap": true,
		"declarationMap": true,
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true
	}
}
