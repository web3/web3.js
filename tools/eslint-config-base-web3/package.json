{
	"name": "eslint-config-base-web3",
	"version": "0.1.0",
	"description": "Base eslint config for Web3 project.",
	"author": "ChainSafe Systems",
	"private": true,
	"license": "LGPL-3.0",
	"keywords": [
		"eslint",
		"ethereum",
		"eth",
		"web3",
		"web3js",
		"web3.js",
		"blockchain"
	],
	"homepage": "https://github.com/ChainSafe/web3.js#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ChainSafe/web3.js.git"
	},
	"bugs": {
		"url": "https://github.com/ChainSafe/web3.js/issues"
	},
	"engines": {
		"node": ">=12",
		"npm": ">=6.12.0"
	},
	"main": "ts.js",
	"scripts": {
		"lint": "eslint --cache --cache-strategy content --ext .js,.ts .",
		"lint:fix": "eslint --fix --ext .js,.ts .",
		"format": "prettier --write '**/*'"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"eslint": "^8.26.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-jest": "^26.6.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"eslint-plugin-no-null": "^1.0.2"
	},
	"devDependencies": {
		"eslint-plugin-deprecation": "^1.3.2",
		"eslint-plugin-header": "^3.1.1",
		"typescript": "^4.7.4"
	}
}
