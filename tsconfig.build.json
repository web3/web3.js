{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "target": "es2020",
        "module": "commonjs",
        "declaration": true,
        "moduleResolution": "node",
        "newLine": "lf",
        "noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"pretty": true,
		"removeComments": true,
        "sourceMap": true,
        "composite": true,
        "declarationMap": true,
        "strict": true,
    }
}
